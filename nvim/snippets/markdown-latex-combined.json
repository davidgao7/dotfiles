{
  "latex-inline-math": {
    "prefix": "dm",
    "body": [
      "$${1:math}$$"
    ],
    "description": "LaTeX inline math block"
  },
  "latex-display-math": {
    "prefix": "dmath",
    "body": [
      "$$",
      "${1:equation}",
      "$$"
    ],
    "description": "LaTeX display math block"
  },
  "latex-bmatrix": {
    "prefix": "bmat",
    "body": [
      "\\begin{bmatrix}",
      "  ${1:1} & ${2:2} \\\\",
      "  ${3:3} & ${4:4}",
      "\\end{bmatrix}"
    ],
    "description": "LaTeX bmatrix environment"
  },
  "latex-align": {
    "prefix": "align",
    "body": [
      "\\begin{align}",
      "  ${1:a} &= ${2:b} \\\\",
      "  ${3:c} &= ${4:d}",
      "\\end{align}"
    ],
    "description": "LaTeX align environment"
  },
  "latex-frac": {
    "prefix": "frac",
    "body": [
      "\\frac{${1:num}}{${2:den}}"
    ],
    "description": "LaTeX fraction"
  },
  "latex-sqrt": {
    "prefix": "sqrt",
    "body": [
      "\\sqrt{${1:expression}}"
    ],
    "description": "LaTeX square root"
  },
  "latex-sum": {
    "prefix": "sum",
    "body": [
      "\\sum_{${1:i=0}}^{${2:n}} ${3:expression}"
    ],
    "description": "LaTeX summation"
  },
  "latex-int": {
    "prefix": "int",
    "body": [
      "\\int_{${1:a}}^{${2:b}} ${3:f(x)} \\,dx"
    ],
    "description": "LaTeX integral"
  },
  "latex-case": {
    "prefix": "cases",
    "body": [
      "\\begin{cases}",
      "  ${1:expr_1}, &\\text{if } ${2:cond_1} \\\\",
      "  ${3:expr_2}, &\\text{if } ${4:cond_2}",
      "\\end{cases}"
    ],
    "description": "Piecewise function using cases"
  }
}

